[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/saznetro
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/saznetro
	label-Verified = -1..+1 group user/saznetro
	push = group user/saznetro
	read = group user/saznetro
	rebase = group user/saznetro
	removeReviewer = group user/saznetro
	submit = group user/saznetro
[access "refs/heads/*"]
	create = group user/saznetro
	push = +force group user/saznetro
	pushMerge = group user/saznetro
	read = group user/saznetro
[access "refs/tags/*"]
	push = group user/saznetro
	push = +force group user/saznetro
